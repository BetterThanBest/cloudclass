<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
   <init-param>
   	<param-name>filterParam</param-name>
   	<param-value>111</param-value>
   </init-param>
  	<filter-name>TestFilter</filter-name>
  	<display-name>TestFilter</display-name>
  	<description></description>
  	<filter-class>user.TestFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>TestFilter</filter-name>
  	<url-pattern>/TestFilter</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>specify</servlet-name>
  	<display-name>specify</display-name>
  	<description></description>
  	<servlet-class>user.specify</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>specifytest</servlet-name>
  	<display-name>specifytest</display-name>
  	<description></description>
  	<servlet-class>user.specifytest</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletForward</servlet-name>
  	<display-name>ServletForward</display-name>
  	<description></description>
  	<servlet-class>user.ServletForward</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>forwardExample</servlet-name>
  	<display-name>forwardExample</display-name>
  	<description></description>
  	<servlet-class>user.forwardExample</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>specify</servlet-name>
  	<url-pattern>/user/specify</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>specifytest</servlet-name>
  	<url-pattern>/user/specifytest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletForward</servlet-name>
  	<url-pattern>/ServletForward</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>forwardExample</servlet-name>
  	<url-pattern>/forwardExample</url-pattern>
  </servlet-mapping>
</web-app>
